views:
  - type: table
    name: Table
    filters:
      and:
        - file.ext != "png"
        - file.ext != "webp"
        - file.ext != "jpg"
    order:
      - file.name
      - file.tags
      - file.ctime
    sort:
      - property: file.ctime
        direction: DESC
    columnSize:
      file.name: 241
      file.tags: 780
